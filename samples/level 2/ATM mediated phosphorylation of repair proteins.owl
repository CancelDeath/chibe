        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.biopax.org/release/biopax-level2.owl#catalysis"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.biopax.org/release/biopax-level2.owl#CONTROLLED"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.biopax.org/release/biopax-level2.owl#catalysis"/>
    </owl:disjointWith>
  </owl:Class>
