      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.biopax.org/release/biopax-level2.owl#openControlledVocabulary"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="http://www.biopax.org/release/biopax-level2.owl#xref"/>
    <rdfs:subClassOf>
