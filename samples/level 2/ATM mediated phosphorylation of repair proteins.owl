    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.biopax.org/release/biopax-level2.owl#sequenceFeature"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.biopax.org/release/biopax-level2.owl#confidence"/>
