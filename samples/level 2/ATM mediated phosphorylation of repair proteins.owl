          <owl:DatatypeProperty rdf:about="http://www.biopax.org/release/biopax-level2.owl#ID-VERSION"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
