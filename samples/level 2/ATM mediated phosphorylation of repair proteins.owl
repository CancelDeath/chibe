                    <bp:SYNONYMS rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Tumor suppressor p53</bp:SYNONYMS>
                    <bp:SYNONYMS rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Antigen NY-CO-13</bp:SYNONYMS>
                    <bp:XREF>
                      <bp:unificationXref rdf:ID="CPATH-LOCAL-87829">
                        <bp:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >Q16808</bp:ID>
